{
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^2.8.8"
  },
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\""
  },
  "lint-staged": {
    "**/*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
